{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/home",
      "dest": "/public/index.html"
    },
    {
      "src": "/about",
      "dest": "/public/pages/Aboutus.html"
    },
    {
      "src": "/booking",
      "dest": "/public/pages/booking.html"
    },
    {
      "src": "/profile",
      "dest": "/public/pages/profile.html"
    },
    {
      "src": "/login",
      "dest": "/public/pages/login.html"
    },
    {
      "src": "/signup",
      "dest": "/public/pages/signup.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
